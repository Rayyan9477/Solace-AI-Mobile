{
  "name": "solace-ai-enterprise",
  "version": "2.0.0",
  "description": "Enterprise-grade Solace AI Mobile - Advanced Mental Health Platform",
  "main": "App.enterprise.js",
  "scripts": {
    "start": "expo start --config app.enterprise.json",
    "start:enterprise": "expo start --config app.enterprise.json --metro-config metro.config.enterprise.js",
    "android": "expo run:android --config app.enterprise.json",
    "ios": "expo run:ios --config app.enterprise.json",
    "web": "expo start --web --config app.enterprise.json",
    "test": "jest --config jest.config.enterprise.js",
    "test:watch": "jest --watch --config jest.config.enterprise.js",
    "test:coverage": "jest --coverage --config jest.config.enterprise.js",
    "test:e2e": "playwright test --config playwright.config.enterprise.js",
    "test:e2e:headed": "playwright test --headed --config playwright.config.enterprise.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --config .eslintrc.enterprise.js",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix --config .eslintrc.enterprise.js",
    "type-check": "tsc --noEmit",
    "build:web": "expo export --platform web --config app.enterprise.json",
    "build:android": "eas build --platform android --profile enterprise",
    "build:ios": "eas build --platform ios --profile enterprise",
    "prebuild": "expo prebuild --config app.enterprise.json",
    "prepare": "husky install",
    "audit:security": "npm audit --audit-level moderate",
    "audit:deps": "depcheck",
    "bundle:analyze": "ANALYZE_BUNDLE=true expo export --platform web --config app.enterprise.json",
    "performance:test": "npm run build:web && lighthouse http://localhost:8080 --output json --output html",
    "enterprise:deploy": "npm run test && npm run lint && npm run build:web",
    "enterprise:validate": "npm run type-check && npm run test:coverage && npm run lint"
  },
  "dependencies": {
    "@expo/config-plugins": "~9.0.0",
    "@expo/metro-config": "~0.19.0",
    "@expo/metro-runtime": "~4.0.1",
    "@expo/prebuild-config": "^8.0.0",
    "@paper-design/shaders-react": "^0.0.46",
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@react-native-community/blur": "^4.4.1",
    "@react-native-community/cli": "^20.0.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "4.5.5",
    "@react-native-material/core": "^1.3.7",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/react": "~18.3.12",
    "animejs": "^4.1.3",
    "axios": "^1.7.7",
    "crypto-js": "^4.2.0",
    "expo": "~52.0.0",
    "expo-av": "~15.0.2",
    "expo-build-properties": "~0.13.2",
    "expo-crypto": "~14.0.2",
    "expo-file-system": "~18.0.12",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-image-picker": "^16.1.4",
    "expo-linear-gradient": "~14.0.2",
    "expo-secure-store": "~14.0.1",
    "expo-speech": "~13.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.4",
    "framer-motion": "^12.23.12",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-paper": "^5.14.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "^15.8.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "~0.19.12",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.24.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
