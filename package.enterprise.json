  "repository": {
    "type": "git",
    "url": "https://github.com/solace-ai/enterprise-mobile.git"
  },
  "bugs": {
    "url": "https://github.com/solace-ai/enterprise-mobile/issues"
  },
  "homepage": "https://solace-ai.com",
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --config .eslintrc.enterprise.js",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run enterprise:validate"
    }
  }
}