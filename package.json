{
  "name": "solace-ai-mobile",
  "version": "1.0.0",
  "description": "Solace AI Mobile - Your Empathetic Digital Confidant",
  "main": "App.tsx",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:ci": "jest --ci --coverage",
    "test:e2e": "detox test -c ios.sim.debug",
    "test:all": "npm run test:coverage",
    "test:playwright": "playwright test",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:mobile": "playwright test --project=iphone-14-pro --project=samsung-galaxy-s21 --project=pixel-7",
    "test:playwright:desktop": "playwright test --project=edge-desktop",
    "test:playwright:report": "playwright show-report",
    "test:playwright:frontend": "set SOLACE_BASE_URL=http://localhost:8081&& playwright test tests/comprehensive-app-test.spec.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "theme-preview": "cd theme-preview && npm start",
    "theme-preview:build": "cd theme-preview && npm run build",
    "setup": "npm install --legacy-peer-deps && cd theme-preview && npm install && cd ..",
    "dev": "concurrently \"npm start\" \"npm run theme-preview\"",
    "expo:install": "npx install-expo-modules@latest",
    "prebuild": "expo prebuild",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/prebuild-config": "~54.0.8",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "5.0.1",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@react-navigation/stack": "^6.4.1",
    "@types/react": "~19.1.10",
    "crypto-js": "^4.2.0",
    "expo": "^54.0.33",
    "expo-apple-authentication": "~8.0.8",
    "expo-auth-session": "~7.0.10",
    "expo-av": "~16.0.8",
    "expo-camera": "~17.0.10",
    "expo-crypto": "~15.0.8",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linear-gradient": "~15.0.8",
    "expo-secure-store": "~15.0.8",
    "expo-speech": "~14.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-sqlite": "~16.0.10",
    "expo-status-bar": "~3.0.9",
    "expo-updates": "~29.0.16",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-native": "0.81.5",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-vector-icons": "^10.3.0",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.7.2",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.13",
    "babel-jest": "^29.7.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "detox": "^20.22.5",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^13.0.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-native-a11y": "^3.5.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.17",
    "lint-staged": "^15.2.10",
    "metro-react-native-babel-transformer": "^0.77.0",
    "prettier": "^3.3.3",
    "react-test-renderer": "19.1.0"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "@types/react": "~19.1.10"
  },
  "resolutions": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "@types/react": "~19.1.10"
  }
}
