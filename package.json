{
  "name": "solace-ai-mobile",
  "version": "1.0.0",
  "description": "Solace AI Mobile - Your Empathetic Digital Confidant",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "theme-preview": "cd theme-preview && npm start",
    "theme-preview:build": "cd theme-preview && npm run build",
    "test:accessibility": "node scripts/test-accessibility-comprehensive.js",
    "test:theme": "node scripts/comprehensive-theme-test.js",
    "test:integration": "node scripts/final-comprehensive-test.js",
    "test:all": "npm run test:accessibility && npm run test:theme && npm run test:integration",
    "setup": "npm install --legacy-peer-deps && cd theme-preview && npm install && cd ..",
    "dev": "concurrently \"npm start\" \"npm run theme-preview\"",
    "expo:install": "npx install-expo-modules@latest",
    "prebuild": "expo prebuild"
  },
  "dependencies": {
    "expo": "~51.0.28",
    "react": "18.2.0",
    "react-native": "0.74.5",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@reduxjs/toolkit": "^1.9.7",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-reanimated": "~3.10.1",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "3.31.1",

    "react-native-linear-gradient": "^2.8.3",
    "react-native-svg": "15.2.0",
    "axios": "^1.6.2",
    "expo-permissions": "~14.4.0",
    "expo-speech": "~12.0.2",
    "react-native-paper": "^5.12.3",
    "lottie-react-native": "6.7.0",
    "@react-native-community/slider": "4.5.2",
    "expo-haptics": "~13.0.1",
    "expo-linear-gradient": "~13.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/eslint-config": "^0.74.0",
    "@react-native/metro-config": "^0.74.0",
    "@react-native/typescript-config": "^0.74.0",
    "@types/react": "~18.2.79",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.76.8",
    "prettier": "^2.4.1",
    "react-test-renderer": "18.2.0",
    "typescript": "~5.3.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
