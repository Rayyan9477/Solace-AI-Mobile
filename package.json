{
  "name": "solace-ai-mobile",
  "version": "1.0.0",
  "description": "Solace AI Mobile - Your Empathetic Digital Confidant",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:ci": "jest --ci --coverage",
    "test:e2e": "detox test -c ios.sim.debug",
    "test:accessibility": "node scripts/test-accessibility-comprehensive.js",
    "test:theme": "node scripts/comprehensive-theme-test.js",
    "test:integration": "node scripts/final-comprehensive-test.js",
    "test:all": "npm run test:accessibility && npm run test:theme && npm run test:integration && npm run test:coverage",
    "test:ui": "node tests/ui/ui-test-runner.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "theme-preview": "cd theme-preview && npm start",
    "theme-preview:build": "cd theme-preview && npm run build",
    "setup": "npm install --legacy-peer-deps && cd theme-preview && npm install && cd ..",
    "dev": "concurrently \"npm start\" \"npm run theme-preview\"",
    "expo:install": "npx install-expo-modules@latest",
    "prebuild": "expo prebuild",
    "prepare": "husky install",
    "sync:figma": "node scripts/fetch-figma-data.js && node scripts/figma-component-analyzer.js"
  },
  "dependencies": {
    "@expo/config-plugins": "~9.0.0",
    "@expo/metro-config": "~0.19.0",
    "@expo/metro-runtime": "~4.0.1",
    "@expo/prebuild-config": "^8.0.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.5",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^2.2.7",
    "@types/react": "~18.3.12",
    "axios": "^1.7.7",
    "expo": "~52.0.0",
    "expo-av": "^15.1.7",
    "expo-file-system": "^18.1.11",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "~14.0.2",
    "expo-speech": "~13.0.1",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-updates": "~0.27.4",
    "hermes-parser": "^0.29.1",
    "lottie-react-native": "7.1.0",
    "metro": "^0.81.0",
    "metro-config": "^0.81.0",
    "metro-resolver": "^0.81.0",
    "prop-types": "^15.8.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.0",
    "react-native-paper": "^5.12.5",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-vector-icons": "^10.2.0",
    "react-native-web": "~0.19.12",
    "react-redux": "^9.1.2",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.7.2",
    "@types/jest": "^29.5.13",
    "babel-jest": "^29.7.0",
    "detox": "^20.22.5",
    "eslint": "^8.57.1",
    "eslint-config-universe": "^13.0.0",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.0-preview.3",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "react-test-renderer": "18.3.1"
  }
}
