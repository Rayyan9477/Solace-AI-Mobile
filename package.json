{
  "name": "solace-ai-mobile",
  "version": "1.0.0",
  "description": "Solace AI Mobile - Your Empathetic Digital Confidant",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "test:ci": "jest --ci --coverage",
    "test:e2e": "detox test -c ios.sim.debug",
    "test:accessibility": "node scripts/test-accessibility-comprehensive.js",
    "test:theme": "node scripts/comprehensive-theme-test.js",
    "test:integration": "node scripts/final-comprehensive-test.js",
    "test:all": "npm run test:accessibility && npm run test:theme && npm run test:integration && npm run test:coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "theme-preview": "cd theme-preview && npm start",
    "theme-preview:build": "cd theme-preview && npm run build",
    "setup": "npm install --legacy-peer-deps && cd theme-preview && npm install && cd ..",
    "dev": "concurrently \"npm start\" \"npm run theme-preview\"",
    "expo:install": "npx install-expo-modules@latest",
    "prebuild": "expo prebuild",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/config-plugins": "~10.1.1",
    "@expo/metro-config": "~0.20.0",
    "@expo/metro-runtime": "~5.0.4",
    "@expo/prebuild-config": "~9.0.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/slider": "4.5.6",
    "@react-native/js-polyfills": "^0.80.1",
    "@react-navigation/bottom-tabs": "^6.6.1",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/stack": "^6.4.1",
    "@reduxjs/toolkit": "^1.9.7",
    "axios": "^1.6.2",
    "expo": "^53.0.18",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-linear-gradient": "~14.1.5",
    "expo-speech": "~13.1.7",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-updates": "~0.28.16",
    "lottie-react-native": "7.2.2",
    "metro": "^0.82.0",
    "metro-config": "^0.82.5",
    "metro-resolver": "^0.82.5",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-paper": "^5.12.3",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-vector-icons": "^9.2.0",
    "react-native-web": "^0.20.0",
    "react-redux": "^8.1.3",
    "redux-persist": "^6.0.0",
    "styled-components": "^6.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "babel-jest": "^29.7.0",
    "detox": "^20.19.3",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "react-test-renderer": "18.2.0"
  }
}
