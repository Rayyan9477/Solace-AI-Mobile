# Production Environment Variables for Solace AI Mobile

# API Configuration
API_BASE_URL=https://api.solace-ai.com
API_TIMEOUT=20000

# Environment
NODE_ENV=production
EXPO_ENV=production

# Analytics & Tracking
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true

# Feature Flags
ENABLE_VOICE_CHAT=true
ENABLE_CRISIS_DETECTION=true
ENABLE_OFFLINE_MODE=true

# External Services (Replace with actual keys)
SENTRY_DSN=https://your-sentry-dsn@sentry.io/project-id
AMPLITUDE_API_KEY=your-amplitude-api-key
FIREBASE_CONFIG=your-firebase-config

# Mental Health Services
CRISIS_HOTLINE_NUMBER=988
EMERGENCY_CONTACT_EMAIL=crisis@solace-ai.com

# App Configuration
APP_VERSION=1.0.0
MIN_SUPPORTED_VERSION=1.0.0